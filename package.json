{
  "name": "responsive-design",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
	"scripts": {
    "dev": "ts-node --project tsconfig.server.json ./server.ts",
    "build": "next build && tsc --project tsconfig.server.json",
    "start": "node server.js -p $PORT",
    "typeorm": "cross-env TS_NODE_PROJECT=tsconfig.ts-node.json TS_NODE_TRANSPILE_ONLY=true ts-node ./node_modules/typeorm/cli.js",
    "migration": "npm run typeorm migration:run",
    "prepare": "husky install",
    "lint": "eslint . --ext ts --ext tsx --ext js",
    "tsc": "tsc"
  },
	"dependencies": {
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "matchmedia-polyfill": "^0.3.2",
    "next": "^11.1.0",
    "next-seo": "^4.24.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "ts-node": "^10.0.0",
    "typescript": "^4.3.2"
  },
	"devDependencies": {
    "@babel/core": "^7.14.3",
    "@babel/plugin-proposal-decorators": "^7.14.2",
    "@next/eslint-plugin-next": "^11.0.0",
    "@types/cors": "^2.8.10",
    "@types/express": "^4.17.12",
    "@types/node": "^16.0.0",
    "@types/react": "^17.0.8",
    "@types/react-dom": "^17.0.5",
    "@typescript-eslint/eslint-plugin": "^4.28.0",
    "@typescript-eslint/parser": "^4.28.0",
    "autoprefixer": "^10.2.6",
    "eslint": "^7.29.0",
    "eslint-config-next": "^11.0.1",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^7.0.1",
    "postcss": "^8.3.0",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-preset-env": "^6.7.0",
    "react-icons": "^4.2.0",
    "tailwindcss": "^2.1.2"
  }
}
